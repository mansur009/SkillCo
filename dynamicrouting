// src/App.js
import React from 'react';
import { Routes, Route } from 'react-router-dom';
import HomePage from './pages/HomePage';
import SearchResultsPage from './pages/SearchResultsPage';
import TradespersonProfilePage from './pages/TradespersonProfilePage'; // Import
import './App.css';

function App() {
  return (
    <div className="App">
      <Routes>
        <Route path="/" element={<HomePage />} />
        <Route path="/search-results" element={<SearchResultsPage />} />
        <Route path="/tradesperson/:tradespersonId" element={<TradespersonProfilePage />} /> {/* Add dynamic route */}
      </Routes>
    </div>
  );
}

export default App;